{
  "plugins": ["no-floating-promise"],
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "no-floating-promise/no-floating-promise": 2,
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "cjs": "always",
        "mjs": "always"
      }
    ]  
  }
}
